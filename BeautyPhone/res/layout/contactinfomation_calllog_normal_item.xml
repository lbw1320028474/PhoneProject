<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="75dp"
    android:background="#ffffff" >

    <ImageView
        android:id="@+id/contactinfomation_calllog_normal_calltype"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_centerVertical="true"
        android:layout_marginLeft="20dp"
        android:scaleType="fitXY"
        android:src="@drawable/ic_call_incoming_normal" />

    <TextView
        android:id="@+id/contactinfomation_calllog_normal_calltime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="30dp"
        android:layout_toRightOf="@+id/contactinfomation_calllog_normal_calltype"
        android:gravity="center_vertical"
        android:text="22:26"
        android:textColor="#000000"
        android:textSize="@dimen/text_size_l" />

    <TextView
        android:id="@+id/contactinfomation_calllog_normal_callderact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="30dp"
        android:layout_toRightOf="@+id/contactinfomation_calllog_normal_calltime"
        android:gravity="center_vertical"
        android:text="6粉32秒"
        android:textSize="@dimen/text_size_xs" />

    <TextView
        android:id="@+id/contactinfomation_calllog_normal_callsimtype"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:gravity="center_vertical"
        android:text="中国移动"
        android:textSize="@dimen/text_size_xs" />

</RelativeLayout>